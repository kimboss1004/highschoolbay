<li>
  <%= link_to group_category_path(current_user.group, object) do %>
     <p><%= object.name %></p>
  <% end %>


  <ul>
  <% if object.sub_categories.any? %>
    
      <% object.sub_categories.each do |category_child| %>
        <%= render 'shared/digs/school_dig', object: category_child %>
      <% end %>  

  <% end %>

  <li class="gray-space-divider"><i></i><p>Custom Categories</p></li>

  <% if object.gategories.any? %>
      <% object.gategories.each do |gategory_child| %>
        <%= render 'shared/digs/gategory_dig', object: gategory_child %>
      <% end %>  
  <% end %>

    <li class="gategory-button">
      <%= link_to "New", new_group_gategory_path(current_user.group) + "?category_id=#{object.id}" %>
    </li>
  </ul>  

</li>
